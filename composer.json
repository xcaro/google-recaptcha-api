{
    "name": "xcaro/laravel-recaptcha-api",
    "description": "reCaptcha for Laravel 5",
    "keywords": ["laravel", "recaptcha"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nhan Vo",
            "email": "tinhnhanvh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "~5.6",
        "illuminate/contracts": "~5.6",
        "illuminate/support": "~5.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "xcaro\\Recaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xcaro\\Recaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "Recaptcha": "xcaro\\Recaptcha\\Facades\\Recaptcha"
            }
        }
    }
}
